<h1> New Todo List</h1>
<div class="row">
  <div class="col-md-8">
    <%= form_for @list do |f| %>
      <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name, class: 'form-control', placeholder: "Enter List Name" %>
      </div>
        <div class="form group">
          <%= f.submit "Save", class: 'btn btn-success' %>
        <% end %>
      </div>
    </div>
  </div>